<UserControl
    x:Class="HomeView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:deneme"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <StackPanel>
        <Label Content="{Binding CompanyId}" Foreground="Black" />
        <Label Content="{Binding Token}" Foreground="Black" />
        <ItemsControl ItemsSource="{Binding RolId}">
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <Label Content="{Binding}" Foreground="Black" />
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>


    </StackPanel>



</UserControl>
